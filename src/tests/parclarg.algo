algorithme procedure parc_larg
parametres locaux
   arbrebinaire b
variables
   entier i
   file f                               /* f stocke les arbres */
debut
   f <- filevide
   f <- enfiler(f,b)                    /* stockage de l'arbre b */
   tant que non(estvide(f)) faire
      b <- premier(f)                   /* recuperation du 1er arbre stocke */
      f <- defiler(f)                   /* liberer la file de cet arbre */
      ecrire(contenu(racine(b)))        /* affichage du noeud racine de l'arbre
recupere */
   si g(b)<>arbre-vide alors
      f <- enfiler(f,g(b))              /* stockage du sous-arbre gauche de b */
   fin si
   si d(b)<>arbre-vide alors
      f <- enfiler(f,d(b))              /* stockage du sous-arbre droit de b */
   fin si
   fin tant que
fin algorithme procedure parc_larg

